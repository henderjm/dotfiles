[include]
  path = ~/.custom_gitconfig
[duet "env"]
	git-author-initials = mjh
	git-author-name = Mark Hender
	git-author-email = mhender@pivotal.io
	mtime = 1508158203
	git-committer-initials = 
	git-committer-name = 
	git-committer-email = 
[core]
	pager = diff-so-fancy | less --tabs=4 -RFX
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 227
	frag = magenta bold
	commit = 227 bold
	old = red bold
	new = green bold
	whitespace = red reverse
